{
  "name": "siren-enrouten",
  "version": "1.0.0",
  "description": "A koa route initialization and configuration module.",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --harmony --require should --reporter spec",
    "test-win": "set NODE_ENV=test & mocha --harmony --require should --reporter spec",
    "test-cov": "NODE_ENV=test node --harmony ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --require should",
    "test-cov-win": "set NODE_ENV=test & node --harmony ./node_modules/istanbul-harmony/lib/cli cover ./node_modules/mocha/bin/_mocha -- --require should",
    "test-travis": "NODE_ENV=test node --harmony ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- --require should"
  },
  "repository": "git://github.com/TerenceZ/siren-enrouten.git",

  "keywords": [
    "koa",
    "node",
    "js",
    "javascript",
    "routing"
  ],
  "author": [
    "Erik Toth <ertoth@paypal.com>",
    "TerenceZ <texvnars@gmail.com>"
  ],
  "licenses": [
    {
      "type": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  ],
  "devDependencies": {
    "istanbul-harmony": "^0.3.1",
    "koa": "^0.14.0",
    "mocha": "^2.1.0",
    "should": "^4.4.2",
    "supertest": "^0.15.0"
  },
  "dependencies": {
    "caller": "^1.0.0",
    "debuglog": "^1.0.1",
    "siren-router": "^1.0.7"
  }
}
